{
  "manifest_version": 3,
  "name": "T3 Chat Keyboard Navigation",
  "version": "1.1.0",
  "description": "Enhances keyboard accessibility on T3 Chat",
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "content_scripts": [
    {
      "matches": ["https://t3.chat/*"],
      "js": ["main.js"],
      "run_at": "document_end"
    }
  ]
} 
